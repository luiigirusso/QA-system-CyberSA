arch:Router1 a d3f:Router ;
    rdfs:label "Router 1" .

### LAN2 DEVICES
arch:WirelessAP a d3f:WirelessAccessPoint ;
    rdfs:label "Wireless AP" .
arch:Laptop1 a d3f:LaptopComputer ;
    rdfs:label "Laptop 1" ;
    arch:wireless_connection arch:WirelessAP .
arch:Laptop2 a d3f:LaptopComputer ;
    rdfs:label "Laptop 2" ;
    arch:wireless_connection arch:WirelessAP .
arch:Desktop1 a d3f:DesktopComputer ;

arch:LAN1 a d3f:Network ;
    rdfs:label "LAN 1" ;
    arch:contains arch:POP3Server1 ;
    arch:contains arch:DNSServer ;
    arch:contains arch:SMTPServer1 ;
    arch:contains arch:Router1 .

arch:LAN2 a d3f:Network ;
    rdfs:label "LAN 2" ;
    arch:contains arch:WirelessAP ;
    arch:contains arch:Laptop1 ;
    arch:contains arch:Laptop2 ;
    arch:contains arch:Mobile ;
    arch:contains arch:Firewall ;
    arch:contains arch:Desktop1 .

arch:Remote a d3f:Network ;
    rdfs:label "Remote Network" ;
    arch:contains arch:VPNServer ;
    arch:contains arch:Desktop2 .

### LAN1 DEVICES
arch:POP3Server1 a arch:POP3Server; 
